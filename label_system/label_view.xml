<?xml version="1.0"?>
<openerp>
    <data>
        <!--product.product-->
        <record model="ir.ui.view" id="view_product_product_label_data_form">
            <field name="name">product.product.label.form</field>
            <field name="model">product.product</field>
            <field name="priority" eval="40"/>
            <field name="arch" type="xml">
                <form string="Product label" version="7.0">        
                    <sheet>
                        <group col="4" colspan="4">
                            <field name="name" />
                            <field name="default_code" />
                            <notebook colspan="4">
                                <page string="Description">
                                    <group col="4" colspan="4">
                                        <field name="label_frame" />
                                        <field name="label_fabric_color" />
                                    </group>
                                </page>
                                <page string="Value">
                                    <group col="4" colspan="4">
                                        <field name="q_x_pack" />
                                    </group>
                                </page>
                                <page string="Code">
                                    <group col="4" colspan="4">
                                        <field name="ean13" string="EAN13"/>
                                        <field name="ean8" string="EAN8"/>
                                        <field name="ean13_mono" string="EAN13 single"/>
                                        <field name="ean8_mono" string="EAN8 single"/>
                                    </group>
                                </page>
                            </notebook>
                        </group>
                    </sheet>        
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="view_product_product_label_data_tree">
            <field name="name">product.product.label.tree</field>
            <field name="model">product.product</field>
            <field name="priority" eval="40"/>
            <field name="arch" type="xml">
                <tree string="Product lavel">
                    <field name="name" />
                    <field name="default_code" />
                    <field name="q_x_pack" />
                    <field name="label_frame" />
                    <field name="label_fabric_color" />
                    <field name="ean13" />
                    <field name="ean8" />
                </tree>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_product_product_label_data">
            <field name="name">Product data label</field>
            <field name="res_model">product.product</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_product_product_label_data_tree"/>
            <field name="context">{'form_view_ref': 'label_system.view_product_product_label_data_form'}</field>
            <field name="domain">[]</field>
        </record>
        <!--label.layout-->
        <record model="ir.ui.view" id="view_label_layout_form">
            <field name="name">label.layout.form</field>
            <field name="model">label.layout</field>
            <field name="arch" type="xml">
                <form string="Label layout" version="7.0">        
                    <sheet>
                        <group col="4" colspan="4">
                            <field name="code"/>
                            <field name="name" />

                            <field name="width" />
                            <field name="height" />
                            
                            <field name="right" />
                            <field name="left" />
                            
                            <field name="top" />
                            <field name="continue" />

                            <separator string="Note:" colspan="4"/>
                            <field name="note" nolabel="1" colspan="4"/>
                        </group>
                     </sheet>        
                  </form>
              </field>
          </record>                                          
        <record model="ir.ui.view" id="view_label_layout_tree">
            <field name="name">label.layout.tree</field>
            <field name="model">label.layout</field>
            <field name="arch" type="xml">
                <tree string="Label layout" colors="blue:continue==True">
                    <field name="continue" />
                    <field name="code"/>
                    <field name="name" />
                    <field name="width" />                    
                    <field name="height" />
                    <field name="right" />
                    <field name="left" />                    
                    <field name="top" />
                    <field name="note" />
                </tree>
            </field>
        </record>      
        <record model="ir.actions.act_window" id="action_label_layout">
            <field name="name">Label layout</field>
            <field name="res_model">label.layout</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_label_layout_tree"/>
            <field name="context">{}</field>
            <field name="domain">[]</field>
        </record>

        <record model="ir.ui.view" id="view_label_label_form">
            <field name="name">label.label.form</field>
            <field name="model">label.label</field>
            <field name="arch" type="xml">
                <form string="Label" version="7.0">        
                    <header>
                        <button name="test_report_label" string="Test label" type="object" attrs="{'invisible': [('report_id','=',False)]}" icon="gtk-print"/>
                        
                        <button string="Confirmed" name="trigger_draft_confirmed" type="workflow" states="draft" icon="STOCK_APPLY"/>
                        <button string="Disable" name="trigger_confirmed_disabled" type="workflow" states="confirmed" icon="gtk-no"/>
                        <button string="Enable" name="trigger_disabled_confirmed" type="workflow" states="disabled" icon="gtk-yes"/>
                        <field name="state" widget="statusbar" />
                    </header>
                    <sheet>
                        <group col="4" colspan="4">
                            <field name="code"/>
                            <field name="name" />
                            
                            <field name="type" />
                            <field name="filename" readonly="1"/>
                            
                            <field name="layout_id" />
                            <field name="report_id" readonly="1"/>

                            <field name="partner_id" />
                            <field name="lang_id" />

                            <notebook colspan="4">
                                <page string="Extra info">
                                    <separator string="Description:" colspan="4"/>
                                    <field name="description" nolabel="1" colspan="4"/>

                                    <separator string="Note:" colspan="4"/>
                                    <field name="note" nolabel="1" colspan="4"/>
                                </page>
                                <page string="Fast label">
                                    <separator string="Fast label:" colspan="4"/>
                                    <field name="fast_ids" nolabel="1" colspan="4" context="{'form_embedded': True}"/>
                                </page>
                            </notebook>
                        </group>
                     </sheet>        
                  </form>
              </field>
          </record>                                          
                                    
        <record model="ir.ui.view" id="view_label_label_tree">
            <field name="name">label.label.tree</field>
            <field name="model">label.label</field>
            <field name="arch" type="xml">
                <tree string="Label" colors="red:report_id==False;grey:state=='disabled';blue:state=='draft';black:state=='confirmed'"> 
                    <field name="code"/>       
                    <field name="name" />
                    <field name="description"/>
                    <field name="filename"/>
                    <field name="layout_id" />                    
                    <field name="partner_id" />
                    <field name="lang_id" />
                    <field name="report_id" />
                    <field name="type"/>
                    <field name="state" />
                    <button string="Confirmed" name="trigger_draft_confirmed" type="workflow" states="draft" icon="STOCK_APPLY"/>
                    <button string="Disable" name="trigger_confirmed_disabled" type="workflow" states="confirmed" icon="gtk-no"/>
                    <button string="Enable" name="trigger_disabled_confirmed" type="workflow" states="disabled" icon="gtk-yes"/>                        

                    <button name="test_report_label" string="Test label" type="object" attrs="{'invisible': [('report_id','=',False)]}" icon="gtk-print"/>
                </tree>
            </field>
        </record> 
        <record model="ir.ui.view" id="view_label_label_search">
            <field name="name">label.label.search</field>
            <field name="model">label.label</field>
            <field name="arch" type="xml">
                <search string="Label" version="7.0">
                    <field name="code"/>        
                    <field name="name" />
                    <field name="description"/>
                    <field name="filename"/>
                    <field name="partner_id" />
                    <field name="lang_id" />
                    <field name="layout_id" />                    
                    <field name="type"/>
                    <field name="note"/>               

                    <filter name="state_draft" string="Draft state" domain="[('state','=','draft')]"/>
                    <filter name="state_confirmed" string="Confirmed state" domain="[('state','=','confirmed')]"/>
                    <filter name="state_disabled" string="Disabled state" domain="[('state','=','disabled')]"/>

                    <group expand="0" string="Group By...">
                        <filter name="group_type" string="Type" context="{'group_by': 'type'}"/>
                        <filter name="group_partner" string="Partner" context="{'group_by': 'partner_id'}"/>
                        <filter name="group_lang" string="Lang" context="{'group_by': 'lang_id'}"/>
                        <filter name="group_layout" string="Layout" context="{'group_by': 'layout_id'}"/>
                        <filter string="Status" context="{'group_by': 'state'}" />
                    </group>                
                </search>
            </field>
        </record>
             
        <record model="ir.actions.act_window" id="action_label_label">
            <field name="name">Label</field>
            <field name="res_model">label.label</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_label_label_tree"/>
            <field name="context">{}</field>
            <field name="domain">[('state', 'in', ('confirmed', 'draft'))]</field>
        </record>
        <record model="ir.actions.act_window" id="action_label_label_draft">
            <field name="name">Label to confirm</field>
            <field name="res_model">label.label</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_label_label_tree"/>
            <field name="context">{}</field>
            <field name="domain">[('state', '=', 'draft')]</field>
        </record>
        <record model="ir.actions.act_window" id="action_label_label_disabled">
            <field name="name">Label disabled</field>
            <field name="res_model">label.label</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_label_label_tree"/>
            <field name="context">{}</field>
            <field name="domain">[('state', '=', 'disabled')]</field>
        </record>
        
        <!--label.label.job-->
        <!--TODO search view-->
        <record model="ir.ui.view" id="view_label_label_job_form">
            <field name="name">label.label.job.form</field>
            <field name="model">label.label.job</field>
            <field name="arch" type="xml">
                <form string="Label job" version="7.0">        
                    <header>
                        <button name="set_fast_label_on" string="Set Fast" type="object" icon="star-on" attrs="{'invisible': [('fast','=',True)]}" help="Fast label is changeable, MRP readonly because generated from procedure"/>
                        <button name="set_fast_label_off" string="Set MRP" type="object" icon="star-off" attrs="{'invisible': [('fast','=',False)]}" help="Fast label is changeable, MRP readonly because generated from procedure"/>

                        <button name="print_fast_label" string="Print" type="object" icon="gtk-print" />
                    </header>                    
                    <sheet>
                        <group col="4" colspan="4">
                            <field name="fast" invisible="1"/>                        
                            <field name="label_id" invisible="context.get('form_embedded', False)" attrs="{'readonly': [('fast','=',False)]}"/>
                            <field name="record_data_counter" attrs="{'readonly': [('fast','=',False)]}" />
                            <field name="type" attrs="{'readonly': [('fast','=',False)]}" />
                            <field name="report_id" readonly="1" attrs="{'readonly': [('fast','=',False)]}"/>
                            <field name="lang_id" widget="selection" attrs="{'readonly': [('fast','=',False)]}"/>
                            <field name="layout_id" widget="selection" attrs="{'readonly': [('fast','=',False)]}"/>
                            <field name="record_print_counter_pack_total" attrs="{'readonly': [('fast','=',False)]}"/>
                            
                            <notebook colspan="4">
                                <page string="Link data">
                                    <group col="4" colspan="4">
                                        <button name="generate_text_data_from_linked" string="Generate" type="object" icon="gtk-execute" attrs="{'invisible': [('fast','=',False)]}"/>
                                        <newline/>
                                        <field name="product_id" attrs="{'readonly': [('fast','=',False)]}" />
                                        <field name="partner_id" attrs="{'readonly': [('fast','=',False)]}" />
                                        <field name="address_id" attrs="{'readonly': [('fast','=',False)]}" />
                                        <field name="order_id" attrs="{'readonly': [('fast','=',False)]}" />
                                        <field name="line_id" attrs="{'readonly': [('fast','=',False)]}" />
                                        <field name="mrp_id" attrs="{'readonly': [('fast','=',False)]}" />
                                    </group>
                                </page>
                                
                                <page string="Product anagrafic">
                                    <group col="8" colspan="4"> 
                                        <separator string="Text" colspan="8"/>
                                        <field name="record_print_code" string="Code" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_code" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_code" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_code_partner" string="Partner code" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_code_partner" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_code_partner" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_description" string="Description" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_description" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_description" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_description_partner" string="Partner description" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_description_partner" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_description_partner" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_frame" string="Frame" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_frame" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_frame" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_fabric" string="Fabric" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_fabric" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_fabric" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <separator string="Numeric" colspan="8"/>
                                        <field name="record_print_q_x_pack" string="Q. x pack" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_q_x_pack" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_q_x_pack" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_q_x_pallet" string="Q. x pallet" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_q_x_pallet" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_q_x_pallet" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_dimension" string="Dimension" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_dimension" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_dimension" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_volume" string="Volume" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_volume" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_volume" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_weight_net" string="Weight net" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_weight_net" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_weight_net" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        
                                        <field name="record_print_weight_lord" string="Weight lord" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_weight_lord" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_weight_lord" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_parcel" string="Parcel" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_parcel" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_parcel" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_price" string="Price" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_price" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_price" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_price_uom" string="Price UOM" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_price_uom" nolabel="1" attrs="{'readonly': [('fast','=',False)]}" colspan="2"/>

                                        <separator string="Static" colspan="8"/>
                                        <field name="record_data_text1" attrs="{'readonly': [('fast','=',False)]}" colspan="4" string="Text 1"/>
                                        <field name="record_data_text2" attrs="{'readonly': [('fast','=',False)]}" colspan="4" string="Text 2"/>
                                        <field name="record_data_text3" attrs="{'readonly': [('fast','=',False)]}" colspan="4" string="Text 3"/>
                                    </group>                    
                                </page>
                                
                                <page string="EAN">
                                    <group col="4" colspan="4"> 
                                        <group col="2" colspan="2"> 
                                            <separator string="EAN13" colspan="2"/>
                                            <field name="record_print_ean13" string="EAN 13" attrs="{'readonly': [('fast','=',False)]}"/>
                                            <field name="record_data_ean13" attrs="{'readonly': [('fast','=',False)]}"/>
                                            <field name="record_data_ean13_mono" attrs="{'readonly': [('fast','=',False)]}"/>
                                        </group>

                                        <group col="2" colspan="2"> 
                                            <separator string="EAN8" colspan="2"/>
                                            <field name="record_print_ean8" string="EAN 8" attrs="{'readonly': [('fast','=',False)]}"/>
                                            <field name="record_data_ean8" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                            <field name="record_data_ean8_mono" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        </group>
                                    </group>
                                </page>
                                
                                <page string="Production">                                            
                                    <group col="8" colspan="4"> 
                                        <field name="record_print_line" string="Line" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_line" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_line" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_period" string="Period" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_period" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_period" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_lot" string="Lot" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_lot" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_lot" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                    </group>
                                </page>
                                
                                <page string="Order">
                                    <group col="8" colspan="4"> 
                                        <field name="record_print_order_ref" string="Order ref." attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_order_ref" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_order_ref" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_order_date" string="Order date." attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_order_date" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_order_date" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>

                                        <field name="record_print_destination_code" string="Destination code" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_string_destination_code" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_data_destination_code" nolabel="1" attrs="{'readonly': [('fast','=',False)]}"/>
                                    </group>
                                </page>
                                <page string="Image">
                                    <group col="6" colspan="4"> 
                                        <field name="record_print_company_logo" string="Print company logo" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_print_partner_logo" string="Print partner logo" attrs="{'readonly': [('fast','=',False)]}"/>
                                        <field name="record_print_linedrawing" string="Print linedrawing" attrs="{'readonly': [('fast','=',False)]}"/>
                                    </group>
                                </page>
                            </notebook>
                        </group>
                     </sheet>        
                  </form>
              </field>
          </record>                                          
        <record model="ir.ui.view" id="view_label_label_job_tree">
            <field name="name">label.label.job.tree</field>
            <field name="model">label.label.job</field>
            <field name="arch" type="xml">
                <tree string="Label job" colors="blue:fast==True">
                    <field name="fast" invisible="1"/>
                    <field name="label_id" invisible="context.get('form_embedded', False)"/>
                    <field name="report_id" />
                    <field name="layout_id" />
                    <field name="type" />
                    <field name="record_data_code" />
                    <field name="record_data_description" />
                    <field name="record_data_counter" />
                    
                    <field name="product_id" />
                    <field name="mrp_id" />                    
                    <field name="partner_id" />
                    <field name="address_id" />
                    <field name="order_id" />
                    <field name="line_id" />

                    <button name="set_fast_label_on" string="Set Fast" type="object" icon="star-on" attrs="{'invisible': [('fast','=',True)]}" help="Fast label is changeable, MRP readonly because generated from procedure"/>
                    <button name="set_fast_label_off" string="Set MRP" type="object" icon="star-off" attrs="{'invisible': [('fast','=',False)]}" help="Fast label is changeable, MRP readonly because generated from procedure"/>

                    <button name="print_fast_label" string="Print" type="object" icon="gtk-print"/>
                </tree>
            </field>
        </record>    
        <record model="ir.ui.view" id="view_label_label_job_search">
            <field name="name">label.label.job.search</field>
            <field name="model">label.label.job</field>
            <field name="arch" type="xml">
                <search string="Label job" version="7.0">        
                    <field name="record_data_code" />
                    <field name="record_data_description" />
                    <field name="label_id" />
                    <field name="report_id" />
                    <!--<field name="layout_id" />
                    <field name="type" />-->
                    
                    <field name="product_id" />
                    <field name="mrp_id" />                    
                    <field name="partner_id" filter_domain="[('partner_id.name','=',self)]"/>
                    <field name="address_id" domain="[('is_address', '=', True)]" filter_domain="[('partner_id.name','=',self)]"/>
                    <field name="order_id" />
                    <field name="line_id" />

                    <filter name="fast_on" string="Fast label" domain="[('fast','=',True)]"/>
                    <group expand="0" string="Group By...">
                        <filter name="group_label" string="label" context="{'group_by': 'label_id'}"/>
                        <filter name="group_report" string="report" context="{'group_by': 'report_id'}"/>
                        <!--change related for type and layout for group by clause-->
                        
                        <filter name="group_product" string="Product" context="{'group_by': 'product_id'}"/>
                        <filter name="group_mrp" string="Production" context="{'group_by': 'mrp_id'}"/>
                        <filter name="group_partner" string="Partner" context="{'group_by': 'partner_id'}"/>
